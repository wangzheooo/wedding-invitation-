<view class="container">
  <swiper class="home-swiper" indicator-dots="true" circular="true" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" bindchange="swiperChange">
    <block wx:for-items="{{lunboData}}" wx:key="{{lunboData.lunbo}}">
      <swiper-item>
        <image src="{{item.imageUrlFinally}}" class="slide-image" style="width: {{imagewidth}}px; height: {{imageheight}}px;" bindload="imageLoad"></image>
      </swiper-item>
    </block>
  </swiper>
</view>